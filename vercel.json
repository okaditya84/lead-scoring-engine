{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "REDIS_URL": "",
    "GROQ_API_KEY": ""
  },
  "functions": {
    "main.py": {
      "maxDuration": 60
    }
  }
}
