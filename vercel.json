{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "REDIS_URL": "",
    "GROQ_API_KEY": "",
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "main.py": {
      "maxDuration": 60
    }
  }
}
